<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#FF00b3a2" />
            <stroke
                android:width="1dp"
                android:color="#FF009488" />
            <corners
                android:radius="19dp" />
            <padding
                android:left="20dp"
                android:top="20dp"
                android:right="20dp"
                android:bottom="20dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#FF00b3a2"
                android:endColor="#FF009488"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#FF009488" />
            <corners
                android:radius="20dp" />
            <padding
                android:left="20dp"
                android:top="20dp"
                android:right="20dp"
                android:bottom="20dp" />
        </shape>
    </item>

</selector>
